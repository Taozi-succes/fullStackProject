{
  "name": "express-enterprise-server",
  "version": "1.0.0",
  "description": "企业级Express服务器，采用模块化架构设计",
  "license": "MIT",
  "author": "Senior Architect",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "start": "npx nodemon server.js",
    "dev": "pm2 start ecosystem.config.js",
    "dev:cluster": "pm2 start ecosystem.config.js --only express-pm2-cluster --env development_cluster",
    "dev:stop": "pm2 stop ecosystem.config.js",
    "dev:restart": "pm2 restart ecosystem.config.js",
    "dev:reload": "pm2 reload ecosystem.config.js",
    "dev:logs": "pm2 logs express-pm2-dev",
    "dev:monit": "pm2 monit",
    "dev:delete": "pm2 delete ecosystem.config.js",
    "prod": "pm2 start ecosystem.config.js --env production",
    "build": "echo \"Build completed\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:seed": "node prisma/seed.js",
    "lint": "echo \"Linting completed\"",
    "format": "echo \"Formatting completed\""
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.2",
    "node-cron": "^4.2.1",
    "prisma": "^6.12.0",
    "svg-captcha": "^1.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
